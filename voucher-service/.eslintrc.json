{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "spread": true
    }
  },
  "globals": {
    "process": true
  },
  "env": {
    "node": true,
    "jest": true
  },
  "rules": {
    "no-console": 0
  }
}
